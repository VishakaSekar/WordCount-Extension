<monitor>    <name>com.appdynamics.extensions.wordcounter.WordCounter</name>    <enabled>true</enabled>    <type>managed</type>    <description>Monitors Word Count metrics</description>    <monitor-configuration></monitor-configuration>    <monitor-run-task>        <execution-style>periodic</execution-style>        <execution-frequency-in-seconds>60</execution-frequency-in-seconds>        <name>WordCount Monitor Run Task</name>        <display-name>WordCount Monitor Task</display-name>        <description>WordCount Monitor Task</description>        <type>java</type>        <execution-timeout-in-secs>60</execution-timeout-in-secs>        <task-arguments>            <!-- config file-->            <argument name="config-file" is-required="true" default-value="monitors/WordCounter/config.yml" />        </task-arguments>        <java-task>            <classpath>WordCountExtension.jar</classpath>            <impl-class>com.appdynamics.extensions.wordcounter.WordCounter</impl-class>        </java-task>    </monitor-run-task></monitor>